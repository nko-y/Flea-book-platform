/* components/bookStack/bookStack.wxss */
.oneBlock{
  width: 100%;
  height: 592rpx;
}

.oneShare{
  width: 93%;
  height: 90%;
  margin: auto;
  border-radius: 20rpx;
  position: relative;
}

.oneImg{
  width: 100%;
  height: 100%;
  border-radius: 20rpx;
  position: absolute;
  top: 0rpx;
  left: 0rpx;
  z-index: -1;
}

.shadow{
  position: absolute;
  top: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.1);
  border-radius: 20rpx;
}
